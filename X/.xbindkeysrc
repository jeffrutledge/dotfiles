"sh -c "pactl set-sink-volume @DEFAULT_SINK@ +5%; pactl set-sink-mute @DEFAULT_SINK@ false; pkill -SIGRTMIN+10 i3blocks""
    m:0x0 + c:123
    XF86AudioRaiseVolume

"sh -c "pactl set-sink-volume @DEFAULT_SINK@ -5%; pactl set-sink-mute @DEFAULT_SINK@ false; pkill -SIGRTMIN+10 i3blocks""
    m:0x0 + c:122
    XF86AudioLowerVolume

"sh -c "pactl set-sink-mute @DEFAULT_SINK@ toggle; pkill -SIGRTMIN+10 i3blocks""
    m:0x0 + c:121
    XF86AudioMute

"xbacklight -ctrl acpi_video0 -inc 33"
    m:0x0 + c:233
    XF86MonBrightnessUp

"xbacklight -ctrl acpi_video0 -dec 33"
    m:0x0 + c:232
    XF86MonBrightnessDown
