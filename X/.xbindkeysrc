"sh -c "pactl set-sink-volume @DEFAULT_SINK@ +2% ; pactl set-sink-mute @DEFAULT_SINK@ false""
    m:0x0 + c:123
    XF86AudioRaiseVolume

"sh -c "pactl set-sink-volume @DEFAULT_SINK@ -2% ; pactl set-sink-mute @DEFAULT_SINK@ false""
    m:0x0 + c:122
    XF86AudioLowerVolume

"pactl set-sink-mute @DEFAULT_SINK@ toggle"
    m:0x0 + c:121
    XF86AudioMute

"xbacklight -ctrl acpi_video0 -inc 33"
    m:0x0 + c:233
    XF86MonBrightnessUp

"xbacklight -ctrl acpi_video0 -dec 33"
    m:0x0 + c:232
    XF86MonBrightnessDown
