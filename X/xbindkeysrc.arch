"sh -c "pactl set-sink-mute @DEFAULT_SINK@ false ; pactl set-sink-volume @DEFAULT_SINK@ +5%""
    m:0x0 + c:123
    XF86AudioRaiseVolume

"sh -c "pactl set-sink-mute @DEFAULT_SINK@ false ; pactl set-sink-volume @DEFAULT_SINK@ -5%""
    m:0x0 + c:122
    XF86AudioLowerVolume

"pactl set-sink-mute @DEFAULT_SINK@ toggle"
    m:0x0 + c:121
    XF86AudioMute

"xbacklight -inc 10"
    m:0x0 + c:233
    XF86MonBrightnessUp

"xbacklight -dec 10"
    m:0x0 + c:232
    XF86MonBrightnessDown
